{
  "name": "poc",
  "version": "1.0.0",
  "private": false,
  "workspaces": [
    "backend",
    "fe-vite"
  ],
  "scripts": {
    "postinstall": "npm run build -w backend",
    "build": "npm run build -w backend",
    "dev": "concurrently \"npm:dev:backend\" \"npm:dev:fe-vite\"",
    "dev:backend": "npm run dev -w backend",
    "dev:fe-vite": "npm run dev -w fe-vite"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
